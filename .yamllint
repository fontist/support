---
rules:
  document-start: disable
  brackets:
    level: warning
    max-spaces-inside: 1
    min-spaces-inside: 1
    min-spaces-inside-empty: 0
    max-spaces-inside-empty: 1
  line-length:
    max: 140
    allow-non-breakable-words: true
    allow-non-breakable-inline-mappings: true
    level: warning
